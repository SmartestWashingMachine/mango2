{
  "name": "mango",
  "version": "2.0.0",
  "description": "Mangone",
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "test": "jest --env=jsdom",
    "start": "electron ./public/electron.js",
    "tsc-i": "tsc --init",
    "build": "webpack --config ./webpack.config.js",
    "buildprod": "webpack --config ./webpack.config.prod.js",
    "bstart": "npm run build && npm run start",
    "opack": "electron-builder --dir",
    "pack": "npm run buildprod && npm run opack",
    "pretty": "prettier --write \"./src/**/*.{ts,tsx}\"",
    "webbuild": "webpack --config ./webpack.config.webapp.js",
    "knip": "knip"
  },
  "author": "Washed Octopus",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@electron-forge/cli": "^6.0.4",
    "@electron-forge/maker-deb": "^6.0.4",
    "@electron-forge/maker-squirrel": "^6.0.4",
    "@electron-forge/maker-zip": "^6.0.4",
    "@faker-js/faker": "^8.4.1",
    "@jest/globals": "^29.7.0",
    "@testing-library/react": "^15.0.5",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.3",
    "@types/react-test-renderer": "^18.3.0",
    "@types/screenshot-desktop": "^1.12.0",
    "babel-jest": "^29.7.0",
    "copyfiles": "^2.4.1",
    "css-loader": "^6.7.1",
    "electron": "^30.0.1",
    "electron-builder": "^24.13.3",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "knip": "^2.43.0",
    "node-loader": "^2.0.0",
    "prettier": "^2.8.8",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-test-renderer": "^18.1.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.0",
    "typescript": "^4.6.4",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@fontsource/roboto": "^4.5.8",
    "@mui/icons-material": "^5.15.15",
    "@mui/lab": "^5.0.0-alpha.81",
    "@mui/material": "^5.15.15",
    "@mui/x-tree-view": "^7.3.1",
    "@types/react-resizable": "^3.0.3",
    "@types/react-transition-group": "^4.4.5",
    "@types/sharp": "^0.30.2",
    "@types/uuid": "^8.3.4",
    "bufferutil": "^4.0.8",
    "classnames": "^2.3.1",
    "dom-to-image-more": "^2.15.0",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^5.0.0-beta.6",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.0.1",
    "html2canvas": "^1.4.1",
    "looks-same": "^7.3.0",
    "node-fetch": "^3.2.4",
    "react-colorful": "^5.5.1",
    "react-draggable": "^4.4.5",
    "react-dropzone": "^14.2.0",
    "react-resizable": "^3.0.4",
    "react-resize-detector": "^8.0.3",
    "react-spring": "^9.7.3",
    "react-transition-group": "^4.4.5",
    "react-virtuoso": "^4.7.7",
    "screenshot-desktop": "^0.0.0-development",
    "sharp": "^0.32.6",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.5.1",
    "utf-8-validate": "^5.0.10",
    "uuid": "^8.3.2"
  },
  "build": {
    "appId": "mango",
    "win": {
      "target": "NSIS",
      "icon": "./public/Icon.png"
    },
    "extends": null,
    "files": [
      "build/*",
      "public/*"
    ],
    "directories": {
      "buildResources": "build"
    },
    "asar": true,
    "asarUnpack": [
      "node_modules/screenshot-desktop/lib/win32"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    }
  }
}
